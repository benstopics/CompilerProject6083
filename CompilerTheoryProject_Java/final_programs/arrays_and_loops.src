	str2 := "test string 2";
	str3 := "test string 3";
	for(i := 0; i < 30)
		strs[i] := str1;
		strs[i+1] := str2;
		strs[i+2] := str3;
		putInteger(i);
		i := i + 3;
	end for;
	
	for(i := 0; i < 30)
		putInteger(i);
