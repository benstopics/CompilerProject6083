PROGRAM JAKE IS
	global integer JAKE;
	global integer ryan[3];
	global integer zach;

	global procedure if_proc()
		integer declaration;
	begin
		if(true) then
			jake := jake + 1;
		else
			zach := zach + RYAN[2];
		end if;
	end procedure;

	global procedure for_proc()
		integer i;
	begin
		for(i := 0; i < zach)
			ryan[1] := zach + i;
		end for;
	end procedure;
begin
	if_proc();
	for_proc();
end program