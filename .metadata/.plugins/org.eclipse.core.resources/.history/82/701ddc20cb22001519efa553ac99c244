package com.benjamindward.compiler;

public class Main {
	public static void main(String[] args) {
		try {
			String inputFile;
			String outputFile = "a.ll";
			if(args.length > 0)
				inputFile = args[0];
			if(args.length > 1)
				outputFile = args[1];
			new Compiler(inputFile, outputFile);
		} catch (Exception e) {
		}
	}
}
